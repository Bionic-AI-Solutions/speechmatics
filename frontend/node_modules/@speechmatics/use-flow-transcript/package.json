  "license": "MIT",
  "dependencies": {
    "typescript-event-target": "^1.1.1",
    "@speechmatics/flow-client-react": "0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "@types/react": "^18.3.12",
    "rollup-plugin-preserve-directives": "^0.4.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "scripts": {
    "build": "rm -rf dist/ && pnpm -C ../real-time-client build && pnpm rollup -c",
